<view><cu-custom vue-id="1025e4f4-1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">房间分配</view></cu-custom><view class="cu-bar bg-white" style="min-height:100rpx;"><view class="margin-left title text-xl text-bold">{{''+family_info.family_contact_name+'的家庭'}}</view></view><block wx:for="{{member_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-card"><view class="cu-item"><view class=" cu-list menu-avatar"><view class="cu-item padding-left" style="height:220rpx;"><view class="cu-avatar lg margin-left-sm bg-white"><image style="width:120rpx;height:100rpx;" src="{{item.gender==undefined||item.gender==null||item.gender==''?'/static/defaulthead.png':item.gender=='男'?'/static/headm.png':'/static/headf.png'}}"></image></view><view class="content4" style="width:calc(100% - 220rpx);"><view class="flex justify-between"><view class="title text-lg text-bold">{{item.name}}</view><view data-event-opts="{{[['tap',[['onShowDetail',['$0'],[[['member_list','',index]]]]]]]}}" class="cu-btn round lines-blue" bindtap="__e">详情</view></view><view class="text-grey ">{{''+(item.gender+' | '+item.age+'岁 | '+item.tel_num)+''}}</view><view class="solid margin-top-sm"><input class="text-left" style="height:60rpx;" placeholder="请分配房间号" name="input" data-event-opts="{{[['input',[['__set_model',[item.room=='(未分配)'||item.room=='-'?'':item,'room','$event',[]]],['changeRoom',['$event','$0'],[[['member_list','',index]]]]]]]}}" value="{{item.room=='(未分配)'||item.room=='-'?'':item.room}}" bindinput="__e"/></view></view></view></view></view></view></block><view class="justify-between bottom-box"><view class="padding flex flex-direction"><button class="cu-btn bg-blue lg" disabled="{{btn_disabled}}" data-event-opts="{{[['tap',[['onSubmit',['$event']]]]]}}" bindtap="__e">完成分配</button></view></view><view class="{{['cu-modal',modalName=='DetailModal'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">详情</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-light-purple"></text></view></view><scroll-view scroll-y="{{true}}" scroll-with-animation="{{true}}"><view class="cu-form-group"><text class="title">姓名</text><view>{{current_item_info.name}}</view></view><view class="cu-form-group"><view class="title">性别</view><view>{{current_item_info.gender}}</view></view><view class="cu-form-group"><text class="title">年龄</text><view>{{current_item_info.age}}</view></view><view class="cu-form-group"><text class="title">民族</text><view>{{current_item_info.nation}}</view></view><view class="cu-form-group"><text class="title">身份证号码</text><view>{{current_item_info.id_num}}</view></view><view class="cu-form-group"><text class="title">联系电话</text><view>{{current_item_info.tel_num}}</view></view><view class="cu-form-group"><text class="title">家庭地址</text><view>{{current_item_info.address}}</view></view><view class="cu-form-group"><text class="title">工作单位</text><view>{{current_item_info.work_place}}</view></view><view class="cu-form-group"><view class="title">有无病史</view><view>{{current_item_info.has_disease_radio?'有':'无'}}</view></view><view hidden="{{!(current_item_info.has_disease_radio)}}" class="cu-form-group"><text class="title">病史</text><view>{{current_item_info.disease_name}}</view></view><view class="cu-form-group"><view class="title">有无服用药物</view><view>{{current_item_info.has_take_medicine_radio?'有':'无'}}</view></view><view hidden="{{!(current_item_info.has_take_medicine_radio)}}" class="cu-form-group"><text class="title">使用药物</text><view>{{current_item_info.medicine_name}}</view></view></scroll-view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-blue" bindtap="__e">了解</button></view></view></view></view></view>